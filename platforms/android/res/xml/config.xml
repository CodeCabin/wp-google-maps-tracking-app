<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.codecabinlivetrackingapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BackgroundGeolocation">
        <param name="android-package" value="com.transistorsoft.cordova.bggeo.CDVBackgroundGeolocation" />
    </feature>
    <name>WP Google Maps Live Tracking App</name>
    <description>
        A
    </description>
    <author email="perry@codecabin.co.za" href="http://www.wpgmaps.com">
        CodeCabin
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <widget id="com.codecabin.livetrackingapp">
    </widget>
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
